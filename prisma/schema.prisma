datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model track_metadata {
  id        Int     @id @default(autoincrement())
  catalog   String?
  isrc      String?
  iswc      String?
  title     String?
  release   String?
  authorId  Int?
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  posts Post[]
}